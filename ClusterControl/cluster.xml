<?xml version="1.0" encoding="UTF-8"?>
<cluster_config>
  <launcher title="App Launcher">
    <applications>
      <app name="Test RPM Usage">
        <command>rpm</command>
        <tooltip>RPM package management.</tooltip>
        <helpURL>/usr/local/X11R6/lib/X11/doc/html/XvQueryAdaptors.3.html</helpURL>
        <options>
          <group name="RPM Searches">
            <option label="List All" flag="-qa"
                    tooltip="List all installed RPMs." />
            <option label="List qt files" flag="-ql qt"
                    tooltip="List all files installed with qt." />
            <option label="List telnet RPM" flag="-qf /usr/bin/telnet"
                    tooltip="List rpm that contains telnet." />
          </group>
        </options>
      </app>
      <app name="ls">
        <command>ls</command>
        <tooltip>ls</tooltip>
        <helpURL></helpURL>
        <options>
          <group name="Dirs">
            <option label="~/" flag="-l ~/"
                    tooltip="" />
            <option label="/home" flag="-l /home"
                    tooltip="" />
          </group>
        </options>
      </app>
      <app name="Test RPM Usage">
        <command>glxgears</command>
        <tooltip>GLX Gears</tooltip>
        <helpURL></helpURL>
        <options>
          <group name="glxgears">
            <option label="One" flag=""
                    tooltip="" />
            <option label="Two" flag=""
                    tooltip="" />
          </group>
        </options>
      </app>
      <app name="Cow with Gaussian Filter">
        <command>scripts/osgViewer-gaussian</command>
        <tooltip>Load the App Viewer application with the Cow model</tooltip>
        <helpURL>/usr/local/X11R6/lib/X11/doc/html/XvQueryAdaptors.3.html</helpURL>
        <options>
          <option label="Stereo" flag="--stereo"
                  tooltip="Enable stereoscopic rendering" />
        </options>
      </app>
      <app name="Cow with Mitchell Filter">
        <command>scripts/osgViewer-mitchell</command>
        <tooltip>Load the App Viewer application with the Cow model</tooltip>
        <helpURL>http://www.137.org/patrick/work.html</helpURL>
        <options>
          <option label="Stereo" flag="--stereo"
                  tooltip="Enable stereoscopic rendering" />
        </options>
      </app>
      <app name="Cow with cylinder Filter">
        <command>scripts/osgViewer-cylinder</command>
        <tooltip>Load the App Viewer application with the Cow model</tooltip>
        <helpURL>http://www.137.org/patrick/work.html</helpURL>
        <options>
          <option label="Stereo" flag="--stereo"
                  tooltip="Enable stereoscopic rendering" />
        </options>
      </app>
      <app name="Cow with catmull Filter">
        <command>scripts/osgViewer-catmull</command>
        <tooltip>Load the App Viewer application with the Cow model</tooltip>
          <helpURL>http://www.137.org/patrick/work.html</helpURL>
        <options>
          <option label="Stereo" flag="--stereo"
                  tooltip="Enable stereoscopic rendering" />
          </options>
      </app>
      <app name="Another Test App">
          <command>scripts/something</command>
        <tooltip>I am another test application.  Run me!</tooltip>
        <helpURL>http://www.137.org/patrick/resume.html</helpURL>
        <options>
          <option label="Things" flag="-t" tooltip="Enable things" />
          <option label="Features" flag="-f" tooltip="Enable features" />
          </options>
      </app>
      <app name="Pipe test">
        <command>yes</command>
      </app>
    </applications>
    <controls>
    </controls>
    <global_options>
      <group name="VR Juggler Configuration">
         <option label="Simulator" flag="sim.base.jconf sim.wand.mixin.jconf" tooltip="CAVE Configuration" />
         <option label="CAVE" flag="cave.jconf" tooltip="CAVE Configuration" />
         <option label="Power Wall" flag="pwall.jconf" tooltip="Power Wall Configuration" />
      </group>
    </global_options>
  </launcher>
  <cluster_node name="Timmy" hostname="timmy.infiscape.com"/>
  <cluster_node name="slave1" hostname="slave1"/>
</cluster_config>
