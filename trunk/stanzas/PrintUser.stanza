<?xml version="1.0" encoding="UTF-8"?>
<stanza label="Print User" global_options="" tooltip="Prints the name of the user running the process." helpUrl="">
  <command class="Linux">/usr/bin/python</command>
  <command class="Windows XP">C:\Python24\python.exe</command>
  <cwd class="Linux">/</cwd>
  <cwd class="Windows XP">C:\</cwd>
  <arg label="Code" editable="false" class="Linux" flag="-c">import os, pwd; print pwd.getpwuid(os.getuid())[0]</arg>
  <arg label="Code" editable="false" class="Windows XP" flag="-c">import win32api; print win32api.GetUserName()</arg>
</stanza>
